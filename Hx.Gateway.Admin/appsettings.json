{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ProSettings": {
    "NavTheme": "light",
    "Layout": "mix",
    "ContentWidth": "Fluid",
    "FixedHeader": false,
    "FixSiderbar": true,
    "Title": "海星网关服务",
    "PrimaryColor": "daybreak",
    "ColorWeak": false,
    "SplitMenus": false,
    "HeaderRender": true,
    "FooterRender": true,
    "MenuRender": true,
    "MenuHeaderRender": true,
    "HeaderHeight": 64
  },
  "DbSettings": {
    "ConnectionConfigs": [
      {
        "ConfigId": "hxcoregateway",
        "DbType": "Sqlite",
        "ConnectionString": "DataSource=./Hx.Gateway.db",
        "EnableInitDb": true,
        "EnableInitSeed": true,
        "EnableUnderLine": true,
        "EnableSqlLog": true
      }
    ]
  }
}
